<?xml version="1.0" encoding="utf-8"?>

<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="RoundImageViews">
        <attr name="borderWidths" format="reference|dimension">
        </attr>
        <attr name="borderRadius" format="reference|dimension">
        </attr>
        <attr name="borderColor" format="reference|color">
        </attr>
        <attr name="hoverColor" format="reference|color">
        </attr>
    </declare-styleable>

    <!--RatingBar        -->
    <declare-styleable name="RatingBar" tools:ignore="ResourceName">
        <attr name="starImageSize" format="dimension"/>
        <attr name="starImageWidth" format="dimension"/>
        <attr name="starImageHeight" format="dimension"/>
        <attr name="starImagePadding" format="dimension"/>
        <attr name="starCount" format="integer"/>
        <attr name="starEmpty" format="reference"/>
        <attr name="starFill" format="reference"/>
        <attr name="starHalf" format="reference"/>
        <attr name="clickable" format="boolean"/>
        <attr name="halfstart" format="boolean"/>
    </declare-styleable>

    <!--   订单发布问题描述  -->
    <declare-styleable name="MultiLineChooseItemTags">
        <attr name="item_backgroundColor" format="reference|color" />
        <attr name="item_selectedBackgroundColor" format="reference|color" />
        <attr name="item_horizontalSpacing" format="dimension" />
        <attr name="item_verticalSpacing" format="dimension" />
        <attr name="item_horizontalPadding" format="dimension" />
        <attr name="item_verticalPadding" format="dimension" />
        <attr name="item_textColor" format="color" />
        <attr name="item_selectedTextColor" format="reference|color" />
        <attr name="item_textSize" format="dimension" />
        <!-- 边框圆角半径-->
        <attr name="item_radius" format="dimension" />
        <attr name="item_bottomLeftRadius" format="dimension" />
        <attr name="item_bottomRightRadius" format="dimension" />
        <attr name="item_topLeftRadius" format="dimension" />
        <attr name="item_topRightRadius" format="dimension" />
        <!-- 边框颜色-->
        <attr name="item_strokeColor" format="reference|color" />
        <attr name="item_selectedStrokeColor" format="reference|color" />
        <!-- 边框宽度-->
        <attr name="item_strokeWidth" format="dimension" />
        <!-- 是否多选-->
        <attr name="item_multiChooseable" format="boolean" />
        <attr name="item_singleLine" format="boolean" />
        <!--单位dp,为了适应wrap_content用integer-->
        <attr name="item_width" format="integer|enum">
            <enum name="wrap_content" value="-2"/>
            <enum name="match_parent" value="-1"/>
        </attr>
        <attr name="item_height" format="integer|enum">
            <enum name="wrap_content" value="-2"/>
            <enum name="match_parent" value="-1"/>
        </attr>
        <attr name="item_maxEms" format="integer" />
    </declare-styleable>
    <declare-styleable name="Themes">
        <attr name="MultiLineChooseLayoutTagsStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="SwitchView">
        <attr name="hasShadow" format="boolean"/>
        <attr name="primaryColor" format="color"/>
        <attr name="primaryColorDark" format="color"/>
        <attr name="isOpened" format="boolean"/>
    </declare-styleable>
    <item type="id" name="tag_position"></item>
    <item type="id" name="id_tab_txt">888</item>

    <declare-styleable name="MZBannerView">
        <attr name="open_mz_mode" format="boolean"/>
        <attr name="canLoop" format="boolean"/>
        <attr name="indicatorPaddingLeft" format="dimension"/>
        <attr name="indicatorPaddingRight" format="dimension"/>
        <attr name="indicatorPaddingTop" format="dimension"/>
        <attr name="indicatorPaddingBottom" format="dimension"/>
        <attr name="indicatorAlign">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="middle_page_cover" format="boolean"/>
    </declare-styleable>

    <!--CardView   -->
    <attr name="ratio" format="float" />
    <attr name="card_padding" format="dimension" />
    <attr name="card_margin" format="dimension" />
    <attr name="card_loop" format="boolean" />
    <attr name="card_max_offset" format="dimension" />
    <attr name="card_scale_rate" format="float" />

    <declare-styleable name="ElasticCardView">
        <attr name="ratio" />
    </declare-styleable>

    <declare-styleable name="CardViewPager">
        <attr name="card_padding" />
        <attr name="card_margin" />
        <attr name="card_scale_rate" />
        <attr name="card_max_offset" />
        <attr name="card_loop" />
    </declare-styleable>

    <declare-styleable name="BannerLayoutStyle">
        <attr name="selectedIndicatorColor" format="color|reference" />
        <attr name="unSelectedIndicatorColor" format="color|reference" />
        <attr name="indicatorShape" format="enum">
            <enum name="rect" value="0" />
            <enum name="oval" value="1" />
        </attr>

        <attr name="selectedIndicatorHeight" format="dimension|reference" />
        <attr name="selectedIndicatorWidth" format="dimension|reference" />

        <attr name="unSelectedIndicatorHeight" format="dimension|reference" />
        <attr name="unSelectedIndicatorWidth" format="dimension|reference" />

        <attr name="indicatorPosition" format="enum">
            <enum name="centerBottom" value="0" />
            <enum name="rightBottom" value="1" />
            <enum name="leftBottom" value="2" />
            <enum name="centerTop" value="3" />
            <enum name="rightTop" value="4" />
            <enum name="leftTop" value="5" />
        </attr>

        <attr name="indicatorSpace" format="dimension|reference" />
        <attr name="indicatorMargin" format="dimension|reference" />
        <attr name="autoPlayDuration" format="integer|reference" />
        <attr name="scrollDuration" format="integer|reference" />
        <attr name="isAutoPlay" format="boolean|reference"/>
    </declare-styleable>

    <declare-styleable name="myCompoundButton">
        <attr name="res" format="reference" />
        <attr name="BackgroundColor" format="reference"/>
        <attr name="text1" format="string" />
        <attr name="textColor1" format="color" />
        <attr name="textSize1" format="dimension" />
        <attr name="text2" format="string" />
        <attr name="textColor2" format="color" />
        <attr name="textSize2" format="dimension" />
        <attr name="text3" format="string"/>
        <attr name="TextGravity" >
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
            <enum name="center" value="4" />
        </attr>
        <attr name="checked" format="boolean" />
    </declare-styleable>
    <attr name="MyradioButton" format="reference" />

   <!--底部导航栏控件 -->
    <attr name="backColor" format="color" />
    <attr name="backColorPress" format="color" />
    <attr name="backGroundImage" format="reference" />
    <attr name="backGroundImagePress" format="reference" />
    <attr name="iconDrawable" format="reference" />
    <attr name="iconDrawablePress" format="reference" />
    <attr name="textColor" format="color" />
    <attr name="textColorPress" format="color" />
    <attr name="text" format="string" />
    <attr name="textSize" format="float" />

    <declare-styleable name="buttonM">
        <attr name="backColor" />
        <attr name="backColorPress" />
        <attr name="backGroundImage" />
        <attr name="backGroundImagePress" />
        <attr name="textColor" />
        <attr name="textColorPress" />
        <attr name="fillet" format="boolean" />
        <attr name="radius" format="float" />
        <attr name="shape">
            <enum name="rectangle" value="0" />
            <enum name="oval" value="1" />
            <enum name="line" value="2" />
            <enum name="ring" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="buttonExtendM">
        <attr name="backColor" />
        <attr name="backColorPress" />
        <attr name="textColor" />
        <attr name="textColorPress" />
        <attr name="iconDrawable" />
        <attr name="iconDrawablePress" />
        <attr name="text" />
        <attr name="textSize" />
        <attr name="spacing" format="dimension" />
        <attr name="style">
            <enum name="iconLeft" value="0" />
            <enum name="iconRight" value="1" />
            <enum name="iconUp" value="2" />
            <enum name="iconBottom" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="menuItemM">
        <attr name="backColor" />
        <attr name="textColor" />
        <attr name="textColorPress" />
        <attr name="iconDrawable" />
        <attr name="iconDrawablePress" />
        <attr name="text" />
        <attr name="textSize" />
        <attr name="unReadCount" format="integer" />
        <attr name="visibleMore">
            <enum name="visible" value="0x00000000" />
            <enum name="gone" value="0x00000008" />
        </attr>
        <attr name="visibleNew">
            <enum name="visible" value="0x00000000" />
            <enum name="gone" value="0x00000008" />
        </attr>
    </declare-styleable>

    <declare-styleable name="menuM">
        <attr name="backColor" />
        <attr name="textColor" />
        <attr name="textColorPress" />
        <attr name="textSize" />
        <attr name="count" format="integer"/>
    </declare-styleable>
</resources>
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowAnimationStyle">@style/AnimationActivity</item>
    </style>
    <style name="AnimationActivity" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/forward_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/forward_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/backward_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/backward_exit</item>
    </style>
    <style name="Loading_dialog">
        <item name="android:windowFrame">@null</item><!--边框-->
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">true</item><!--半透明-->
        <item name="android:windowNoTitle">true</item><!--无标题-->
        <item name="android:windowBackground">@drawable/shape_dialog_custom_bg</item><!--背景透明-->
        <item name="android:backgroundDimEnabled">false</item><!--模糊-->
        <item name="android:backgroundDimAmount">0.6</item>
    </style>
    <style name="full_screen_dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item><!--无标题-->
        <item name="android:windowIsTranslucent">true</item><!--半透明-->
        <item name="android:windowBackground">@drawable/shape_dialog_custom_bg</item><!--背景透明-->
     <!--   <item name="android:backgroundDimEnabled">false</item>&lt;!&ndash;是否window变暗&ndash;&gt;-->
    </style>
    <style name="share_qrcode_dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item><!--无标题-->
        <item name="android:backgroundDimEnabled">false</item><!--是否window变暗-->
    </style>
    <style name="popup_window_anim">
        <item name="android:windowEnterAnimation">@anim/popwindow_in</item>
        <item name="android:windowExitAnimation">@anim/popwindow_exit</item>
    </style>
    <style name="actionBarTheme" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:textColorPrimary">@color/white</item>
        <item name="actionBarSize">@dimen/margin_width50</item>
    </style>
    <style name="NoTranslucent" parent="AppTheme">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowDisablePreview">true</item>
   </style>
    <style name="customTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="actionBarTheme">@style/actionBarTheme</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="actionBarSize">@dimen/margin_width50</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>
    <!--启动页主题  -->
    <style name="LauncherTheme" parent="AppTheme">
        <item name="android:windowBackground">@drawable/launch_xh</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="FullscreenTheme" parent="AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <!--加载 主题  -->
    <style name="CustomDialog">
        <item name="android:windowFrame">@null</item><!--边框-->
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item><!--半透明-->
        <item name="android:windowNoTitle">true</item><!--无标题-->
        <item name="android:windowBackground">@drawable/shape_dialog_custom_bg</item><!--背景透明-->
        <item name="android:backgroundDimEnabled">false</item><!--模糊-->
        <item name="android:backgroundDimAmount">0.6</item>
    </style>
    <style name="PromptDialogStyle" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item><!-- 边框 -->
        <item name="android:windowIsFloating">true</item><!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item><!-- 半透明 -->
        <item name="android:windowNoTitle">true</item><!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <!--tablayout文字字体  -->
    <style name="MyTabLayoutTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">@dimen/text_18</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="MyTabLayoutTextAppearanceInverse" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/colorAccent</item>
    </style>
    <!-- 上传头像-->
    <style name="Animation.PopupAnimation" parent="android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>
    <style name="CustomTabPageIndicator.Text" parent="android:TextAppearance.Medium">
        <item name="android:typeface">monospace</item>
    </style>

    <!--     订单发布问题描述  -->
    <style name="MultiLineChooseItemTags">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="item_backgroundColor">#FFFFFF</item>
        <item name="item_selectedTextColor">#FFFFFF</item>
        <item name="item_selectedBackgroundColor">@color/colorOrange</item>
        <item name="item_textSize">14sp</item>
        <item name="item_textColor">@color/title_tv</item>
        <item name="item_horizontalSpacing">5dp</item>
        <item name="item_verticalSpacing">5dp</item>
        <item name="item_horizontalPadding">12dp</item>
        <item name="item_verticalPadding">3dp</item>
        <item name="item_radius">5dp</item>
        <item name="item_strokeColor">@color/title_tv</item>
        <item name="item_selectedStrokeColor">@color/colorOrange</item>
        <item name="item_strokeWidth">1px</item>
        <item name="item_multiChooseable">true</item>
        <item name="item_singleLine">true</item>
        <item name="item_width">85</item>
        <item name="item_height">30</item>
    </style>
    <style name="MultiLineChooseItemTags.FlowLayout" parent="MultiLineChooseItemTags">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="item_textColor">#666666</item>
        <item name="item_backgroundColor">#ffffff</item>
        <item name="item_selectedTextColor">#ffffff</item>
        <item name="item_selectedBackgroundColor">@color/colorOrange</item>
        <item name="item_radius">5dp</item>
        <item name="item_strokeColor">#939393</item>
        <item name="item_selectedStrokeColor">@color/colorOrange</item>
        <item name="item_strokeWidth">1px</item>
        <item name="item_horizontalSpacing">15dp</item>
        <!--这里设置固定大小，padding为0-->
        <item name="item_horizontalPadding">10dp</item>
        <item name="item_verticalPadding">5dp</item>
        <!--单位dp-->
        <item name="item_width">wrap_content</item>
        <item name="item_height">wrap_content</item>
        <!--限制tag显示长度,规则查看Textview-->
        <item name="item_maxEms">5</item>
        <!--多选?-->
        <item name="item_multiChooseable">true</item>
    </style>
    <style name="MultiLineChooseItemTags.SingleFlowLayout" parent="MultiLineChooseItemTags">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="item_textColor">#666666</item>
        <item name="item_backgroundColor">#ffffff</item>
        <item name="item_selectedTextColor">#ffffff</item>
        <item name="item_selectedBackgroundColor">@color/colorOrange</item>
        <item name="item_radius">5dp</item>
        <item name="item_strokeColor">#939393</item>
        <item name="item_selectedStrokeColor">@color/colorOrange</item>
        <item name="item_strokeWidth">1px</item>
        <item name="item_horizontalSpacing">15dp</item>
        <!--这里设置固定大小，padding为0-->
        <item name="item_horizontalPadding">10dp</item>
        <item name="item_verticalPadding">5dp</item>
        <!--单位dp-->
        <item name="item_width">wrap_content</item>
        <item name="item_height">wrap_content</item>
        <!--限制tag显示长度,规则查看Textview-->
        <item name="item_maxEms">5</item>
        <!--多选?-->
        <item name="item_multiChooseable">false</item>
    </style>
    <style name="MyRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/selector_my_evalute_ratingbar</item>
        <item name="android:minHeight">60dip</item>
        <item name="android:minWidth">60dip</item>
        <item name="android:maxHeight">60dip</item>
    </style>
    <style name="MatchMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="ActionSheetDialogStyle" parent="android:Theme.Dialog">

        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 浮于Activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- Dialog以外的区域模糊效果 -->
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.5</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- Dialog进入及退出动画 -->
        <item name="android:windowAnimationStyle">@style/ActionSheetDialogAnimation</item>
    </style>
    <style name="PublicSheetDialogStyle" parent="android:Theme.Dialog">
        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 浮于Activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- Dialog以外的区域模糊效果 -->
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.5</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- Dialog进入及退出动画 -->
        <item name="android:windowAnimationStyle">@style/ActionSheetDialogAnimation</item>
    </style>
    <!-- ActionSheet进出动画 -->
    <style name="ActionSheetDialogAnimation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/actionsheet_dialog_in</item>
        <item name="android:windowExitAnimation">@anim/actionsheet_dialog_out</item>
    </style>
    <!-- TopRightMenu -->
    <style name="TRM_ANIM_STYLE">
        <item name="android:windowEnterAnimation">@anim/trm_popup_enter</item>
        <item name="android:windowExitAnimation">@anim/trm_popup_exit</item>
    </style>
    <style name="search_good_small">
        <item name="android:textSize">12dp</item>
    </style>

    <style name="search_good_big">
        <item name="android:textSize">20dp</item>
    </style>

    <style name="dp14">
        <item name="android:textSize">14dp</item>
    </style>

    <style name="dp12">
        <item name="android:textSize">12dp</item>
    </style>

    <!--              shop               -->

    <style name="shopMsbcolorTitleText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/msb_color</item>
        <item name="android:textSize">@dimen/text_width18</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:padding">@dimen/margin_Modules</item>
    </style>
    <style name="shopBlackTitleText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_width18</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:padding">@dimen/margin_Modules</item>
    </style>
    <!-- 分隔线 -->
    <style name="shopTitleLine">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">@color/msb_color</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="shopRouteLine">
        <item name="android:layout_width">2dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@color/shop_grey</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="shopRoutePoint">
        <item name="android:layout_width">@dimen/dp10</item>
        <item name="android:layout_height">@dimen/dp10</item>
        <item name="android:background">@drawable/shop_page_circle_on</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <!-- 分隔线右边的点 -->
    <style name="shopTitleLinePoint">
        <item name="android:layout_width">5dp</item>
        <item name="android:layout_height">5dp</item>
        <item name="android:background">@drawable/shop_page_circle_on</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <!--价格显示-->
    <style name="small_money">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="big_money">
        <item name="android:textSize">16sp</item>
    </style>
    <style name="price">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/msb_color</item>
        <item name="android:textSize">@dimen/text_width18</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_margin">@dimen/margin_Modules</item>
    </style>
    <style name="grid_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/nc_bg</item>
        <item name="android:horizontalSpacing">@dimen/dp4</item>
        <item name="android:numColumns">2</item>
        <item name="android:verticalSpacing">@dimen/dp4</item>
        <item name="android:layout_marginTop">@dimen/dp1</item>
    </style>
    <style name="image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/dp5</item>
    </style>
    <style name="rcl">
        <item name= "android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:requiresFadingEdge">none</item>
       <item name="android:scrollbars">none</item>

    </style>
<style name="black_title_text">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_weight">1</item>
    <item name="android:textSize">@dimen/text_width18</item>
    <item name="android:textColor">@color/black</item>
    <item name="android:layout_height">match_parent</item>
</style>
    <style name="grey_small_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/text_14</item>
        <item name="android:textColor">@color/nc_text</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/margin_Modules</item>
    </style>
    <style name="route_grey_small_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/text_14</item>
        <item name="android:textColor">@color/nc_text</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">@dimen/margin_Modules</item>
    </style>
<style name="editText">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">match_parent</item>
    <item name="android:background">@null</item>
    <item name="android:textSize">@dimen/text_width18</item>
    <item name="android:textColor">@color/black</item>
    <item name="android:paddingLeft">@dimen/margin_Modules</item>
    <item name="android:paddingRight">@dimen/margin_Modules</item>
</style>
</resources>

<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/white"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="@dimen/margin_Modules"
        android:paddingBottom="@dimen/margin_Modules"
        android:gravity="center_horizontal"
        >
        <View
            style="@style/shopTitleLine"/>
        <View
            style="@style/shopTitleLinePoint"
            />
        <TextView
            android:id="@+id/title"
            style="@style/shopMsbcolorTitleText"
            />
        <View
            style="@style/shopTitleLinePoint"
            />
        <View
            style="@style/shopTitleLine"/>
    </LinearLayout>
    <com.msht.minshengbao.custom.CardVMZBanner.MZBannerView
        android:id="@+id/banner"
        android:layout_width="match_parent"
        android:layout_height="@dimen/margin_width160"
        android:layout_marginBottom="@dimen/margin_width12"
        app:canLoop="true"
        app:indicatorAlign="center"
        app:indicatorPaddingBottom="10dp"
        app:indicatorPaddingLeft="10dp"
        app:indicatorPaddingTop="@dimen/margin_width10"
        app:middle_page_cover="true"
        app:open_mz_mode="true"/>
</LinearLayout>
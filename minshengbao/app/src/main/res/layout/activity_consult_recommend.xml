<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_consult_recommend"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.msht.minshengbao.functionActivity.myActivity.ConsultRecommendActivity">
   <include layout="@layout/layout_header"/>
 <com.msht.minshengbao.ViewUI.ViewPagerIndicator
     android:id="@+id/indicator"
     android:layout_width="match_parent"
     android:layout_height="50dp"
     android:background="@color/white">
 </com.msht.minshengbao.ViewUI.ViewPagerIndicator>
    <include layout="@layout/layout_line_gray"/>
    <android.support.v4.view.ViewPager
        android:id="@+id/viewPager"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
    </android.support.v4.view.ViewPager>
</LinearLayout>

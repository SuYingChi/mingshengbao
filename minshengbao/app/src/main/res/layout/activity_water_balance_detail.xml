<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background_gray"
    tools:context="com.msht.minshengbao.functionActivity.waterApp.WaterBalanceDetailActivity">
    <include layout="@layout/layout_white_header" />
    <include layout="@layout/layout_water_balance_detail_type"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/margin_width5"/>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.jcodecraeer.xrecyclerview.XRecyclerView
            android:id="@+id/id_income_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:divider="@null">
        </com.jcodecraeer.xrecyclerview.XRecyclerView>
        <include layout="@layout/layout_no_data_view"
            android:id="@+id/id_noData_view"
            android:visibility="gone"/>
    </FrameLayout>
</LinearLayout>

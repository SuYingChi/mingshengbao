<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_gray"
    android:orientation="vertical"
    tools:context=".functionActivity.lpgActivity.LpgOrderDetailActivity">
    <include layout="@layout/layout_header" />
    <ScrollView
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="0dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_width10"
                android:background="@drawable/shape_change_background"/>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/background_gray">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/margin_width70"
                        android:background="@drawable/shape_change_background"/>
                </LinearLayout>
                <include layout="@layout/layout_lpg_order_detail_header" />
            </FrameLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_width10"/>
            <RelativeLayout
                android:id="@+id/id_layout_delivery"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_width10"
                android:layout_marginStart="@dimen/margin_width10"
                android:layout_marginRight="@dimen/margin_width10"
                android:layout_marginEnd="@dimen/margin_width10"
                android:background="@color/white"
                android:minHeight="@dimen/margin_width80">
                <ImageView
                    android:id="@+id/id_express_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/margin_width15"
                    android:layout_marginStart="@dimen/margin_width15"
                    android:layout_centerVertical="true"
                    android:contentDescription="@string/picture_text"
                    android:src="@drawable/expressage_vehicle_xh"/>
                <TextView
                    android:id="@+id/id_delivery_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/id_express_img"
                    android:layout_toEndOf="@+id/id_express_img"
                    android:layout_marginLeft="@dimen/margin_width15"
                    android:layout_marginStart="@dimen/margin_width15"
                    android:layout_toLeftOf="@+id/id_forward1"
                    android:layout_toStartOf="@+id/id_forward1"
                    android:layout_marginEnd="@dimen/margin_width5"
                    android:layout_marginRight="@dimen/margin_width5"
                    android:textColor="@color/color_9e9e9e"
                    android:textSize="@dimen/text_width14"
                    android:text="@string/delivery_good_tip"/>
                <ImageView
                    android:id="@+id/id_forward1"
                    android:layout_width="@dimen/margin_width15"
                    android:layout_height="@dimen/margin_width15"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_width10"
                    android:layout_marginEnd="@dimen/margin_width10"
                    android:contentDescription="@string/picture_text"
                    android:layout_alignParentEnd="true"
                    android:src="@drawable/forward_2x"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_width10"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_width10"
                android:layout_marginStart="@dimen/margin_width10"
                android:layout_marginRight="@dimen/margin_width10"
                android:layout_marginEnd="@dimen/margin_width10"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="@dimen/margin_width40"
                    android:background="@color/white">
                    <ImageView
                        android:id="@+id/id_site_img"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_width10"
                        android:layout_marginBottom="@dimen/margin_width10"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:layout_centerVertical="true"
                        android:contentDescription="@string/picture_text"
                        android:src="@drawable/site_shop_xh"/>
                    <TextView
                        android:id="@+id/id_tv_siteName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/id_site_img"
                        android:layout_toEndOf="@+id/id_site_img"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:layout_toLeftOf="@+id/id_tv_status"
                        android:layout_toStartOf="@+id/id_tv_status"
                        android:layout_marginEnd="@dimen/margin_width5"
                        android:layout_marginRight="@dimen/margin_width5"
                        android:textColor="@color/title_tv"
                        android:textSize="@dimen/text_width14"
                        android:text="@string/default_text"/>
                    <TextView
                        android:id="@+id/id_tv_status"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/margin_width10"
                        android:layout_marginEnd="@dimen/margin_width10"
                        android:layout_alignParentEnd="true"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/colorOrange"
                        android:text="@string/default_text"/>
                </RelativeLayout>
                <LinearLayout
                    android:id="@+id/id_fiveWeightGas_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                <include layout="@layout/layout_line_gray"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="@dimen/margin_width45"
                    android:background="@color/white">
                    <TextView
                        android:id="@+id/id_bottle_type1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/margin_width15"
                        android:layout_marginTop="@dimen/margin_width15"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/color_383838"
                        android:textStyle="bold"
                        android:text="@string/steel_bottle_type1"/>
                    <TextView
                        android:id="@+id/id_bottle_num1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/id_five_amount"
                        android:layout_toStartOf="@+id/id_five_amount"
                        android:layout_toRightOf="@+id/id_bottle_type1"
                        android:layout_toEndOf="@+id/id_bottle_type1"
                        android:layout_marginStart="@dimen/margin_width20"
                        android:layout_marginLeft="@dimen/margin_width20"
                        android:text="@string/default_value0"/>
                    <TextView
                        android:id="@+id/id_five_amount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="@dimen/margin_width10"
                        android:layout_marginEnd="@dimen/margin_width10"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/colorOrange"
                        android:text="@string/default_value0"/>
                </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/id_fifteenWeightGas_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                <include layout="@layout/layout_line_interval"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="@dimen/margin_width45"
                    android:background="@color/white">
                    <TextView
                        android:id="@+id/id_bottle_type2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/margin_width15"
                        android:layout_marginTop="@dimen/margin_width15"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/color_383838"
                        android:textStyle="bold"
                        android:text="@string/steel_bottle_type2"/>
                    <TextView
                        android:id="@+id/id_bottle_num2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/id_fifteen_amount"
                        android:layout_toStartOf="@+id/id_fifteen_amount"
                        android:layout_toRightOf="@+id/id_bottle_type2"
                        android:layout_toEndOf="@+id/id_bottle_type2"
                        android:layout_marginStart="@dimen/margin_width20"
                        android:layout_marginLeft="@dimen/margin_width20"
                        android:text="@string/default_value0"/>
                    <TextView
                        android:id="@+id/id_fifteen_amount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="@dimen/margin_width10"
                        android:layout_marginEnd="@dimen/margin_width10"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/colorOrange"
                        android:text="@string/default_value0"/>
                </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/id_fiftyWeightGas_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                <include layout="@layout/layout_line_interval"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="@dimen/margin_width45"
                    android:background="@color/white">
                    <TextView
                        android:id="@+id/id_bottle_type3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/margin_width15"
                        android:layout_marginTop="@dimen/margin_width15"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/color_383838"
                        android:textStyle="bold"
                        android:text="@string/steel_bottle_type2"/>
                    <TextView
                        android:id="@+id/id_bottle_num3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/id_fifty_amount"
                        android:layout_toStartOf="@+id/id_fifty_amount"
                        android:layout_toRightOf="@+id/id_bottle_type3"
                        android:layout_toEndOf="@+id/id_bottle_type3"
                        android:layout_marginStart="@dimen/margin_width20"
                        android:layout_marginLeft="@dimen/margin_width20"
                        android:text="@string/default_value0"/>
                    <TextView
                        android:id="@+id/id_fifty_amount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="@dimen/margin_width10"
                        android:layout_marginEnd="@dimen/margin_width10"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/colorOrange"
                        android:text="@string/default_value0"/>
                </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/id_fiveWeightPledge_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                <include layout="@layout/layout_line_interval"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="@dimen/margin_width45"
                    android:background="@color/white">
                    <TextView
                        android:id="@+id/id_pledge_text1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/margin_width15"
                        android:layout_marginTop="@dimen/margin_width15"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/color_383838"
                        android:textStyle="bold"
                        android:text="@string/pledge_five_weight_text"/>
                    <TextView
                        android:id="@+id/id_pledge_amount1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="@dimen/margin_width10"
                        android:layout_marginEnd="@dimen/margin_width10"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/colorOrange"
                        android:text="@string/default_value0"/>
                </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/id_fifteenWeightPledge_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                <include layout="@layout/layout_line_interval"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="@dimen/margin_width45"
                    android:background="@color/white">
                    <TextView
                        android:id="@+id/id_pledge_text2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/margin_width15"
                        android:layout_marginTop="@dimen/margin_width15"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/color_383838"
                        android:textStyle="bold"
                        android:text="@string/pledge_fifteen_weight_text"/>
                    <TextView
                        android:id="@+id/id_pledge_amount2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="@dimen/margin_width10"
                        android:layout_marginEnd="@dimen/margin_width10"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/colorOrange"
                        android:text="@string/default_value0"/>
                </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/id_fiftyWeightPledge_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                <include layout="@layout/layout_line_interval"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="@dimen/margin_width45"
                    android:background="@color/white">
                    <TextView
                        android:id="@+id/id_pledge_text3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/margin_width15"
                        android:layout_marginTop="@dimen/margin_width15"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/color_383838"
                        android:textStyle="bold"
                        android:text="@string/pledge_fifty_weight_text"/>
                    <TextView
                        android:id="@+id/id_pledge_amount3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="@dimen/margin_width10"
                        android:layout_marginEnd="@dimen/margin_width10"
                        android:textSize="@dimen/text_width16"
                        android:textColor="@color/colorOrange"
                        android:text="@string/default_value0"/>
                </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_width10"/>
            <include layout="@layout/layout_lpg_order_total_amount"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_width10"/>
            <RelativeLayout
                android:id="@+id/id_layout_replace_bottle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="@dimen/margin_width40"
                android:layout_marginLeft="@dimen/margin_width10"
                android:layout_marginStart="@dimen/margin_width10"
                android:layout_marginRight="@dimen/margin_width10"
                android:layout_marginEnd="@dimen/margin_width10"
                android:background="@color/white">
                <TextView
                    android:id="@+id/id_bottle_replacement"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/margin_width10"
                    android:layout_marginStart="@dimen/margin_width10"
                    android:layout_marginTop="@dimen/margin_width10"
                    android:layout_marginBottom="@dimen/margin_width10"
                    android:text="@string/self_own_bottle_replacement"
                    android:textColor="@color/color_383838"
                    android:textSize="@dimen/text_width16"/>
                <TextView
                    android:id="@+id/id_retrieveAmount"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="end"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/id_bottle_replacement"
                    android:layout_toEndOf="@+id/id_bottle_replacement"
                    android:layout_toStartOf="@+id/id_forward2"
                    android:layout_toLeftOf="@+id/id_forward2"
                    android:layout_marginRight="@dimen/margin_width10"
                    android:layout_marginEnd="@dimen/margin_width10"
                    android:text="@string/default_value0"
                    android:textColor="@color/deep_green"/>
                <ImageView
                    android:id="@+id/id_forward2"
                    android:layout_width="@dimen/margin_width15"
                    android:layout_height="@dimen/margin_width15"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="@dimen/margin_width10"
                    android:layout_marginRight="@dimen/margin_width10"
                    android:contentDescription="@string/picture_text"
                    android:src="@drawable/forward_2x"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_width10"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_width10"
                android:layout_marginStart="@dimen/margin_width10"
                android:layout_marginRight="@dimen/margin_width10"
                android:layout_marginEnd="@dimen/margin_width10"
                android:background="@color/white"
                android:orientation="vertical">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_width10"
                    android:background="@color/transparent">
                    <TextView
                        android:id="@+id/id_order_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:textSize="@dimen/text_width14"
                        android:text="@string/place_order_no"/>
                    <TextView
                        android:id="@+id/id_tv_orderNo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/id_order_text"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_toEndOf="@+id/id_order_text"
                        android:textSize="@dimen/text_width14"
                        android:text="@string/default_text"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_width10"
                    android:background="@color/transparent">
                    <TextView
                        android:id="@+id/id_time_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:textSize="@dimen/text_width14"
                        android:text="@string/place_time"/>
                    <TextView
                        android:id="@+id/id_place_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/id_time_text"
                        android:layout_marginStart="@dimen/margin_width10"
                        android:layout_marginLeft="@dimen/margin_width10"
                        android:layout_marginBottom="@dimen/margin_width10"
                        android:layout_toEndOf="@+id/id_time_text"
                        android:textSize="@dimen/text_width14"
                        android:text="@string/default_text"/>
                </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_width20"/>
        </LinearLayout>
    </ScrollView>
    <com.msht.minshengbao.ViewUI.ButtonUI.ButtonM
        android:id="@+id/id_btn_cancel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:visibility="gone"
        android:minHeight="@dimen/margin_width45"
        app:backGroundImage="@drawable/shape_change_background"
        app:backColorPress="#F4D6CE"
        android:text="@string/cancel_order"
        app:fillet="true"
        app:shape="rectangle"
        app:textColor="@android:color/white"/>
    <LinearLayout
        android:id="@+id/id_layout_pay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/margin_width45"
        android:baselineAligned="false"
        android:background="@color/transparent"
        android:orientation="horizontal">
        <RelativeLayout
            android:layout_weight="0.7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:minHeight="@dimen/margin_width45">
            <TextView
                android:id="@+id/id_real_amount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="@dimen/margin_width10"
                android:layout_marginEnd="@dimen/margin_width10"
                android:textSize="@dimen/text_width16"
                android:textColor="@color/colorOrange"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/id_real_amount"
                android:layout_toStartOf="@+id/id_real_amount"
                android:layout_marginRight="@dimen/margin_width10"
                android:layout_marginEnd="@dimen/margin_width10"
                android:layout_centerVertical="true"
                android:textSize="@dimen/text_width16"
                android:textColor="@color/color_383838"
                android:text="@string/real_pay_amount"/>
        </RelativeLayout>
        <com.msht.minshengbao.ViewUI.ButtonUI.ButtonM
            android:id="@+id/id_btn_pay"
            android:layout_weight="0.3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/margin_width45"
            android:gravity="center"
            android:text="@string/pay_fee"
            app:backGroundImage="@drawable/shape_change_background"
            app:backColorPress="#F4D6CE"
            app:textColor="@android:color/white"/>
    </LinearLayout>
</LinearLayout>
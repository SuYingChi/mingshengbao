    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:id="@+id/id_serve_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/selector_white_rectangle_change">
        <TextView
            android:id="@+id/tv_serve_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/margin_width44"
